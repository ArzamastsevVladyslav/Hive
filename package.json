{
	"name": "Hive",
	"version": "0.0.2",
	"versionDate": "2018-09-17",
	"author": "hackolade",
	"engines": {
		"hackolade": "2.1.x",
		"hackoladePlugin": "1.1.0"
	},
	"contributes": {
		"target": {
			"applicationTarget": "HIVE",
			"title": "Apache Hive",
			"versions": [
				"2.x",
				"3.x"
			]
		},
		"features": {
			"nestedCollections": false,
			"disablePatternField": false,
			"enableForwardEngineering": true,
			"disableMultipleTypes": false,
			"disableReverseEngineering": false,
			"disableChoices": false,
			"enableJsonType": true
		}
	},
	"description": "Hackolade plugin for Apache Hive"
}
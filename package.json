{
	"name": "Hive",
	"version": "0.1.57",
	"versionDate": "2021-08-10",
	"author": "hackolade",
	"engines": {
		"hackolade": "5.1.3",
		"hackoladePlugin": "1.2.0"
	},
	"contributes": {
		"target": {
			"applicationTarget": "HIVE",
			"title": "Apache Hive",
			"versions": [
				"1.x",
				"2.x",
				"3.x"
			]
		},
		"features": {
            "views": {
                "enabled": true,
                "viewLevel": "model",
                "disablePipelines": true
            },
			"forwardEngineering": {
				"jsonSchema": {
					"keepParentType": {
						"date": {
							"type": "string",
							"format": "date"
						}
					}
				}
			},
			"nestedCollections": false,
			"disablePatternField": false,
			"enableForwardEngineering": true,
			"disableMultipleTypes": false,
			"enableReverseEngineering": true,
			"disableChoices": false,
			"enableJsonType": true,
			"relationships": {
                "compositeRelationships": true                
			},
			"FEScriptCommentsSupported": ["2.x", "3.x"]
		}
	},
	"description": "Hackolade plugin for Apache Hive"
}
